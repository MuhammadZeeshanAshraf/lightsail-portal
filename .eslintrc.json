{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "indent": "off",
        "semi": [
            "error",
            "always"
        ],
        "camelcase": [
            "error",
            {
                "properties": "never"
            }
        ]
    }
}